{
  "type": "default",
  "components": [
    {
      "key": "money",
      "label": "Pack Cash",
      "type": "textfield",
      "validate": {
        "required": true,
        "pattern": "^(?:0|[1-9][0-9]*)\\.[0-9]+$"
      },
      "description": "How much cash do you want to bring?"
    },
    {
      "key": "country",
      "label": "Where are you headed to?",
      "type": "textfield",
      "description": "Country to start a revolution",
      "validate": {
        "required": true,
        "pattern": "America|France"
      }
    },
    {
      "key": "taxiCost",
      "label": "Enter the price of the taxi",
      "type": "textfield",
      "validate": {
        "pattern": "^\\$?\\-?([1-9]{1}[0-9]{0,2}(\\,\\d{3})*(\\.\\d{0,2})?|[1-9]{1}\\d{0,}(\\.\\d{0,2})?|0(\\.\\d{0,2})?|(\\.\\d{1,2}))$|^\\-?\\$?([1-9]{1}\\d{0,2}(\\,\\d{3})*(\\.\\d{0,2})?|[1-9]{1}\\d{0,}(\\.\\d{0,2})?|0(\\.\\d{0,2})?|(\\.\\d{1,2}))$|^\\(\\$?([1-9]{1}\\d{0,2}(\\,\\d{3})*(\\.\\d{0,2})?|[1-9]{1}\\d{0,}(\\.\\d{0,2})?|0(\\.\\d{0,2})?|(\\.\\d{1,2}))\\)$"
      },
      "description": "for tax purposes"
    }
  ]
}